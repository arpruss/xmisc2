<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    android:summary="Activate for the apps you are tweaking in your xposed manager."
    >
 <CheckBoxPreference android:title="Silence Outlook"
    android:summary="Disable sounds (e.g., message sent) in Outlook."
    android:defaultValue="false"
    android:key="outlook_silence" />
-    <CheckBoxPreference android:title="Chrome navigation bar match"
        android:summary="Black navigation bar in Chrome when status bar is darker."
        android:defaultValue="false"
        android:key="chrome_match_navbar" />
    <CheckBoxPreference android:title="Chrome disable tab groups"
        android:summary="Disable tab groups."
        android:defaultValue="false"
        android:key="chrome_kill_tabgroups" />
    <CheckBoxPreference android:title="No wake on plug/unplug"
        android:summary="Disable wake on plug/unplug (reboot if changed). Activate module for System Framework to use."
        android:defaultValue="false"
        android:key="no_wake_on_plug" />
    <CheckBoxPreference android:title="Long press back button menu"
        android:summary="Long press back button activates options menu for legacy apps. Activate module for these apps to use."
        android:defaultValue="false"
        android:key="long_back_menu" />

</PreferenceScreen>